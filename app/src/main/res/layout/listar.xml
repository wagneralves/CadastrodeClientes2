<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:gravity="top"
        android:weightSum="1"
        android:id="@+id/linearLayout2"
        android:baselineAligned="false"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/linearLayout"
        android:layout_toStartOf="@+id/linearLayout"
        android:background="#F5F5F5">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/imageView14"
            android:layout_marginLeft="5dp"
            android:src="@drawable/icone"
            android:layout_gravity="left|center_vertical"
            android:onClick="Sobre" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/tvLIstarTitulo"
            android:id="@+id/textView4"
            android:layout_gravity="left|center_vertical"
            android:textStyle="bold|italic"
            android:layout_marginLeft="5dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="54dp"
        android:gravity="top"
        android:weightSum="1"
        android:layout_alignParentTop="true"
        android:id="@+id/linearLayout"
        android:layout_alignParentRight="true"
        android:baselineAligned="false"
        android:background="#F5F5F5">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="8dp"
            android:layout_weight="0.33"
            android:onClick="CadastroClientes"
            android:src="@drawable/ab_novo" />

        <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:id="@+id/imageView15"
            android:layout_weight="0.33"
            android:src="@drawable/backup"
            android:layout_marginRight="8dp"
            android:layout_gravity="center_vertical|right"
            android:onClick="Backup1" />

        <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:id="@+id/imageView5"
            android:layout_weight="0.33"
            android:src="@drawable/restore"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="8dp"
            android:onClick="Restore1" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/imageView13"
            android:src="@drawable/doe"
            android:layout_weight="0.33"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="8dp"
            android:onClick="Doe" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/imageView17"
            android:layout_weight="0.33"
            android:src="@drawable/compartilhe"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="8dp"
            android:onClick="Compartilhar" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/imageView16"
            android:layout_weight="0.33"
            android:src="@drawable/problema"
            android:layout_marginRight="5dp"
            android:onClick="ApagarTudo"
            android:layout_gravity="center_vertical|right" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/linearLayout2">

        <ListView
            android:id="@+id/ltwDados"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="-20dp"
            android:layout_gravity="center_vertical|bottom"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="4dp" />
    </LinearLayout>

</RelativeLayout>
